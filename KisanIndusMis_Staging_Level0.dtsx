                                                (select sum (tran_amt) from tbaadm.htd,tbaadm.cxl where cxl.chrg_tran_id=htd.tran_id and htd.tran_Date=cxl.chrg_tran_Date and cxl.event_id ='KCC_CC_PROSF_FEE' and htd.part_tran_type='C' and htd.acid=gam.acid and chrg_part_tran_srl_num = part_tran_srl_num and event_type != 'LNKED'  )Processing_Fee_amount_C,
