                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select A.*,CAST (REGEXP_REPLACE(TRIM (existing_bank_name),'[^[a-z,A-Z,.,0-9,[:space:]]]*','') AS VARCHAR2 (80))      AS existing_bank_name_new,CAST (REGEXP_REPLACE(TRIM (TAKEOVER_FROM),'[^[a-z,A-Z,0-9,[:space:]]]*.','') AS VARCHAR2 (80)) AS TAKEOVER_FROM_NEW
from
(SELECT  
                State, 
                Branch_Code, 
                Branch_Name,
                Iworks_ID,
                CIF_ID, 
                Account_No, 
                Account_Name, 
                Scheme_Code, 
                Sanctioned_Limit, 
                Active_DP, 
                Outstanding_Amount,
                (Active_DP-abs(Outstanding_Amount)) Available_Balance,
                Sanction_Date,
                Account_Open_Date, 
                Review_Date, 
                Expiry_Date, 
                INT_RATE, 
                Int_Accrual_Since_Ac_Open, 
                Interest_Amt_Debited, 
                Penal_Amount,
                Pnl_Int_Accrual_Since_Ac_Open,
                Penal_Pcnt,
                (nvl(to_number(Processing_Fee_amount_D),0)-nvl(to_number(Processing_Fee_amount_C),0)) PROCESSING_FEE,
                (CASE WHEN Sanctioned_Limit &gt; 0 then 
				 round((((nvl(to_number(Processing_Fee_amount_D),0)-nvl(to_number(Processing_Fee_amount_C),0))/Sanctioned_Limit)*100),2) else 0 end) Processing_Percentage,
                (nvl(to_number(Insurance_Premium_D),0)-nvl(to_number(Insurance_Premium_C),0)) INSURANCE_FEE,
                Insurance_Proposal_No,
                AC_RM_ID,
                CRM_RM_ID,
                INT_FREQ,
                NXT_INT_DUE_DATE,
                PRIN_FREQ,
                NXT_PRIN_DUE_DATE,
                NEXT_PRIN_DMD,
                RCD_Time,
                Lease_Land_in_Acre,
               Owned_LAnd_in_Acre,
                Crop_Details,
                SB_LINKEDAC_NO,
                SB_LINKEDAC_OPENDT,
                customer_segment_code,
                GL_SUB_HEAD_CODE, 
                Sector_code,
                Sector_code_desc,
                Sub_sector_code,
                Sub_sector_code_desc,
                Borrower_category,
                Advance_Type,
                Type_of_advn_desc,
                Acct_label,
                Weaker_section,
                Weaker_section_desc,
                Free_code_8,
                Free_code_8_desc,
                closure_flg,
                Closure_date,
                EXISTING_AGRI_EXPOSURE,
                EXISTING_BANK_NAME,
                CREDIT_DEVIATION_CODE,
                TAKEOVER,
                TAKEOVER_FROM,
                TAKEOVER_AMT,
                REFERRAL_ECN,
                CROP_INSURANCE,
                AGRI_RM_ECN,
                LEAD_PROVIDED_BY
                
from 
 
                                                (select sol.state_code State, 
