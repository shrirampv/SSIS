from 
tbaadm.gam,tbaadm.gac,tbaadm.sol,tbaadm.eit
where 
gam.acid=gac.acid
and gam.entity_cre_flg='Y'
and gam.bank_id='01' 
and gac.bank_id='01' 
and gam.sol_id=sol.sol_id
and gam.acid=eit.entity_id
and eit.entity_type='ACCNT'
and gam.Sanct_lim &lt;&gt;'0'
and (gam.schm_code in ('OD752','OD753','OD754','OD001','OD761','OD767') OR (gam.schm_code in ('OD650','OD603','OD617','OD652') and gac.free_code_1='RA'))) 


